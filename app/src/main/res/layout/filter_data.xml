<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:background="#2B3B4F"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal|top"
    tools:context=".LinearLayout"
    >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <Button
            android:id="@+id/logout"
            android:text="Logout"
            android:textColor="#ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <Button
            android:id="@+id/myprofile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="My Profile"
            android:textColor="#ffffff"
            android:layout_alignParentRight="true"/>

    </RelativeLayout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        android:paddingTop="150px"
        tools:context=".LinearLayout" >

        <Button
            android:id="@+id/pickstartdate"
            android:text="Start Date"
            android:layout_width="300px"
            android:layout_height="wrap_content"/>
        <Button
            android:id="@+id/pickstarttime"
            android:text="Start Time"
            android:layout_width="300px"
            android:layout_height="wrap_content"/>





    </LinearLayout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        android:paddingTop="25px"
        tools:context=".LinearLayout" >

        <TextView
            android:id="@+id/chosenstartdate"
            android:background="#ffffff"
            android:layout_width="300px"
            android:layout_height="wrap_content"
            android:textSize="40px"
            android:textStyle="bold"
            android:padding="20px"/>

        <TextView
            android:id="@+id/chosenstarttime"

            android:background="#ffffff"
            android:layout_width="300px"
            android:layout_height="wrap_content"
            android:textSize="40px"
            android:textStyle="bold"
            android:padding="20px"/>




    </LinearLayout>


    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        android:paddingTop="50px"
        tools:context=".LinearLayout" >

        <Button
            android:id="@+id/pickenddate"
            android:text="End Date"
            android:layout_width="300px"
            android:layout_height="wrap_content"/>
        <Button
            android:id="@+id/pickendtime"
            android:text="End Time"
            android:layout_width="300px"
            android:layout_height="wrap_content"/>





    </LinearLayout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        android:paddingTop="25px"
        tools:context=".LinearLayout" >

        <TextView
            android:id="@+id/chosenenddate"
            android:background="#ffffff"
            android:layout_width="300px"
            android:layout_height="wrap_content"
            android:textSize="40px"
            android:textStyle="bold"
            android:padding="20px"/>

        <TextView
            android:id="@+id/chosenendtime"

            android:background="#ffffff"
            android:layout_width="300px"
            android:layout_height="wrap_content"
            android:textSize="40px"
            android:textStyle="bold"
            android:padding="20px"/>




    </LinearLayout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        android:paddingTop="50px"
        tools:context=".LinearLayout" >

        <Button
            android:id="@+id/submitfilter"
            android:text="Get Data"
            android:layout_width="300px"
            android:layout_height="wrap_content"/>





    </LinearLayout>



</LinearLayout>